<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Status_for_Path__c</fullName>
    <externalId>false</externalId>
    <formula>IF( 
   ISPICKVAL(Current_Status__c , &quot;Approved&quot;), 
   &quot;Closed&quot;, 
   IF( 
       ISPICKVAL(Current_Status__c , &quot;Denied&quot;), 
       &quot;Closed&quot;, 
       TEXT(Current_Status__c )
   )
)</formula>
    <label>Status for Path</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
